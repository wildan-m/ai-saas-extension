{
  "name": "ai-saas-extension",
  "displayName": "AI SAAS Content Analyzer",
  "version": "1.0.0",
  "description": "Chrome extension that analyzes SAAS application content using AI",
  "author": "Wildan Muhlis",
  "scripts": {
    "dev": "node scripts/simple-dev.js",
    "build": "plasmo build && node scripts/inject-env.js",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.16",
    "chokidar": "^4.0.3",
    "dotenv": "^17.2.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ]
  }
}
